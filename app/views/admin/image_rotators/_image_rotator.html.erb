<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(image_rotator) -%>">
  <span class='title'>
    <%= image_rotator %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_image_rotator_path(image_rotator),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_image_rotator_path(image_rotator),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :'data-confirm' => t('shared.admin.delete.message'),
        :'data-method' => :delete %>
  </span>
</li>
